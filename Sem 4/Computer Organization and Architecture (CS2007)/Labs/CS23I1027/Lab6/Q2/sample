#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x565425875f80 .scope module, "tb_mux4to1" "tb_mux4to1" 2 1;
 .timescale 0 0;
v0x565425888890_0 .var "data_in_reg", 7 0;
v0x565425888980_0 .net "output_y_wire", 1 0, v0x565425888670_0;  1 drivers
v0x565425888a50_0 .var "select_reg", 1 0;
S_0x565425876100 .scope module, "uut" "mux4to1" 2 8, 3 1 0, S_0x565425875f80;
 .timescale 0 0;
    .port_info 0 /INPUT 8 "data_in"
    .port_info 1 /INPUT 2 "select"
    .port_info 2 /OUTPUT 2 "output_y"
v0x5654258542e0_0 .net "data_in", 7 0, v0x565425888890_0;  1 drivers
v0x565425888670_0 .var "output_y", 1 0;
v0x565425888750_0 .net "select", 1 0, v0x565425888a50_0;  1 drivers
E_0x565425876890 .event edge, v0x565425888750_0, v0x5654258542e0_0;
    .scope S_0x565425876100;
T_0 ;
    %wait E_0x565425876890;
    %load/vec4 v0x565425888750_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x565425888670_0, 0, 2;
    %jmp T_0.5;
T_0.0 ;
    %load/vec4 v0x5654258542e0_0;
    %parti/s 2, 0, 2;
    %store/vec4 v0x565425888670_0, 0, 2;
    %jmp T_0.5;
T_0.1 ;
    %load/vec4 v0x5654258542e0_0;
    %parti/s 2, 2, 3;
    %store/vec4 v0x565425888670_0, 0, 2;
    %jmp T_0.5;
T_0.2 ;
    %load/vec4 v0x5654258542e0_0;
    %parti/s 2, 4, 4;
    %store/vec4 v0x565425888670_0, 0, 2;
    %jmp T_0.5;
T_0.3 ;
    %load/vec4 v0x5654258542e0_0;
    %parti/s 2, 6, 4;
    %store/vec4 v0x565425888670_0, 0, 2;
    %jmp T_0.5;
T_0.5 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x565425875f80;
T_1 ;
    %vpi_call 2 16 "$dumpfile", "mux4to1.vcd" {0 0 0};
    %vpi_call 2 17 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x565425875f80 {0 0 0};
    %pushi/vec4 163, 0, 8;
    %store/vec4 v0x565425888890_0, 0, 8;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x565425888a50_0, 0, 2;
    %delay 10, 0;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x565425888a50_0, 0, 2;
    %delay 10, 0;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x565425888a50_0, 0, 2;
    %delay 10, 0;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x565425888a50_0, 0, 2;
    %delay 10, 0;
    %vpi_call 2 32 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x565425875f80;
T_2 ;
    %vpi_call 2 37 "$monitor", "Time = %0t | data_in = %b | select = %b | output_y = %b", $time, v0x565425888890_0, v0x565425888a50_0, v0x565425888980_0 {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb_4to1.v";
    "sample.v";
