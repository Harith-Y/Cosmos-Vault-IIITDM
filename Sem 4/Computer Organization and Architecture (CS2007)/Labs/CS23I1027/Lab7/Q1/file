    %store/vec4 v0x55c00ed82710_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55c00ed827d0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55c00ed82710_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55c00ed827d0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55c00ed82710_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55c00ed827d0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55c00ed82710_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55c00ed827d0_0, 0, 1;
