    .scope S_0x55c00ed6fc30;
T_0 ;
    %vpi_call 2 18 "$dumpfile", "half_adder.vcd" {0 0 0};
    %vpi_call 2 19 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55c00ed6fc30 {0 0 0};
